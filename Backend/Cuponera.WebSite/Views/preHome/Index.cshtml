@model IEnumerable<Cuponera.Entities.preHomeImages>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Active)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.CreationDatetime)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ModificationDatetime)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.DeletionDatetime)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ImagePath)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Latitude)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Longitude)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Active)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.CreationDatetime)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.ModificationDatetime)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.DeletionDatetime)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.ImagePath)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Latitude)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Longitude)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.IdPreHomeImage }) |
            @Html.ActionLink("Details", "Details", new { id=item.IdPreHomeImage }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.IdPreHomeImage })
        </td>
    </tr>
}

</table>
